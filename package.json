{
  "name": "vitify-admin",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit",
    "supportedBrowsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > ./src/supportedBrowsers.ts",
    "type:vuetify": "node ./scripts/vuetify-type.mjs > ./src/vuetify2.d.ts"
  },
  "dependencies": {
    "@mdi/js": "^6.7.96",
    "@vue/composition-api": "^1.6.2",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.27.2",
    "echarts": "^5.3.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "screenfull": "^6.0.1",
    "vue": "^2.6.14",
    "vue-echarts": "^6.0.3",
    "vue-i18n": "^8.27.0",
    "vue-router": "^3.5.4",
    "vuetify": "^2.6.6"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^2.2.4",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.25",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vue/runtime-dom": "^3.2.36",
    "browserslist-useragent-regexp": "^3.0.2",
    "cypress": "^9.7.0",
    "prettier": "^2.6.2",
    "sass": "~1.32.12",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.2",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
    "unplugin-vue2-script-setup": "^0.11.0",
    "vite": "^2.9.9",
    "vite-plugin-vue2": "^2.0.1",
    "vite-plugin-vue2-svg": "^0.1.8",
    "vue-template-babel-compiler": "^1.2.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.35.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not ie>0"
  ]
}

